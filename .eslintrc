{
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": 2,
    "comma-style": [2, "last"],
    "curly": [0, "all"],
    "dot-notation": [2, {
      "allowKeywords": true
    }],
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": [2, "except-parens"],
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 0,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-plusplus": 0,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 0,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-unreachable": 2,
    "no-use-before-define": 2,
    "no-with": 2,
    "quotes": [2, "single"],
    "jsx-quotes": [2, "prefer-double"],
    "react/display-name": 0,
    "react/jsx-boolean-value": [2, "always"],
    "react/jsx-curly-spacing": [2, "never"],
    "react/jsx-max-props-per-line": 0,
    "react/jsx-no-duplicate-props": [2, {
      "ignoreCase": false
    }],
    "react/jsx-no-literals": 0,
    "react/jsx-no-undef": 2,
    "react/jsx-sort-prop-types": 0,
    "react/jsx-sort-props": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-danger": 2,
    "react/no-did-mount-set-state": 2,
    "react/no-did-update-set-state": 2,
    "react/no-multi-comp": 2,
    "react/no-unknown-property": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 2,
    "react/require-extension": [2, {
      "extensions": [".js", ".jsx"]
    }],
    "react/self-closing-comp": 2,
    "react/sort-comp": 0,
    "react/wrap-multilines": [2, {
      declaration: true,
      assignment: true,
      return: true
    }],
    "semi": 2,
    "strict": [2, "never"],
    "valid-typeof": 2,
    "wrap-iife": [2, "inside"]
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ]
}
